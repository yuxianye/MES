    <!--<Thickness x:Key="ControlMarginRight">0 0 5 0</Thickness>-->

    <!--按钮默认的宽度，高度根据按钮内容自适应-->
    <sys:Double x:Key="DefaultButtonWidth">100</sys:Double>
    <sys:Double x:Key="ViewButtonWidth">65</sys:Double>
    <sys:Double x:Key="MapViewButtonWidth">136</sys:Double>

    <sys:Double x:Key="InputControlHeight">27</sys:Double>

    <!--PatientHomeView-->
