<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sysWin="http://schemas.microsoft.com/netfx/2009/xaml/presentation"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!--1.在此设置之后则不用在各个区域里面重复设置相应的值。-->
    <!--2.如果各个区域需要单独设置，则不用在此设置。-->
    <!--3.如果都设置则区域里面的值覆盖本文件的值，-->

    <sysWin:GridLength x:Key="ViewButtonAreaHeight">40</sysWin:GridLength>
    <sysWin:GridLength x:Key="ViewButtonAreaHeight2">65</sysWin:GridLength>

    <sys:Double x:Key="DataPageComboBoxWidth">75</sys:Double>
    <sys:Double x:Key="LeftTreeAutoHideMinWidth">195</sys:Double>

    <sys:Double x:Key="SearchTextBoxWidth">220</sys:Double>
    <sys:Double x:Key="SearchTextBoxWidth2">260</sys:Double>

    <BooleanToVisibilityConverter x:Key="btv" />
    <sys:Int32 x:Key="TitlebarHeight">70</sys:Int32>

    <!--主窗体最小尺寸-->
    <!--<sys:Double x:Key="MainWindowsMinWidth">1366</sys:Double>
    <sys:Double x:Key="MainWindowsMinHeight">739</sys:Double>-->
    <sys:Double x:Key="MainWindowsMinWidth">1024</sys:Double>
    <sys:Double x:Key="MainWindowsMinHeight">690</sys:Double>

    <!--popup窗体尺寸-->
    <sys:Double x:Key="PopupWinodwsWidth">600</sys:Double>
    <sys:Double x:Key="PopupWinodwsWidth2">800</sys:Double>
    <sys:Double x:Key="PopupWinodwsWidth3">400</sys:Double>
    <sys:Double x:Key="PopupWinodwsWidth4">900</sys:Double>

    <sys:Double x:Key="PopupWinodwsHeight">600</sys:Double>
