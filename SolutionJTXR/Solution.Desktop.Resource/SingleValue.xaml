    <sys:Double x:Key="PopupWinodwsHeight3">300</sys:Double>
    <sys:Double x:Key="LoginWindowHeight">450</sys:Double>
    <sys:Double x:Key="LoginWindowWidth">800</sys:Double>

    <!--圆形按钮高河宽-->
    <sys:Double x:Key="CircleButtonWidth">35</sys:Double>
    <sys:Double x:Key="CircleButtonHeight">35</sys:Double>
    <sys:Double x:Key="CircleButtonIconWidth">13</sys:Double>
    <sys:Double x:Key="CircleButtonIconHeight">13</sys:Double>

    <!--popup窗体中内容的总高度-->
    <sysWin:GridLength x:Key="PopupWinodwsContentHeight">525</sysWin:GridLength>
    <sysWin:GridLength x:Key="PopupWinodwsContentHeight2">530</sysWin:GridLength>
    <sysWin:GridLength x:Key="PopupWinodwsContentHeight3">95</sysWin:GridLength>

    <!--popup窗体中某列的宽度，一般分三列，设置首列和尾列，中间列自适应-->
    <sysWin:GridLength x:Key="PopupWinodwsColumn1Width1">160</sysWin:GridLength>
    <sysWin:GridLength x:Key="PopupWinodwsColumn3Width1">40</sysWin:GridLength>

    <!--内容字体大小-->
    <sys:Double x:Key="ContentFontSize">14</sys:Double>

    <sys:Double x:Key="TabItemFontSize">18</sys:Double>

    <!--标题字体大小-->
    <sys:Double x:Key="HeaderFontSize">14</sys:Double>
    <sys:Double x:Key="HeaderFontSize2">18</sys:Double>

    <!--控件的边距-->
    <Thickness x:Key="ControlDefaultMargin">0,0,0,0</Thickness>
    <Thickness x:Key="ControlMargin">0,3,0,3</Thickness>
    <Thickness x:Key="ControlMargin1">5,5,5,5</Thickness>
    <Thickness x:Key="ControlMargin2">2,1,2,1</Thickness>
    <Thickness x:Key="ControlMargin4">0 5 5 0</Thickness>
    <Thickness x:Key="ControlMargin5">5 5 5 0</Thickness>
    <Thickness x:Key="ControlMargin6">5 0 0 0</Thickness>

